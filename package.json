{
  "name": "unordered-editor",
  "version": "1.0.1",
  "description": "a simple web editor",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "run-p build:client:dev build:server:dev start:client:dev start:server:dev:delay",
    "start:server:dev:delay": "run-s copy delay start:server:dev",
    "start:server:dev": "NODE_ENV=development nodemon -w demo-dist/ demo-dist/demo/server/index.js",
    "start:client:dev": "NODE_ENV=development babel-node demo/server/webpack.js",
    "build:server:dev": "NODE_ENV=development babel demo/ -w -d demo-dist/demo/",
    "build:client:dev": "NODE_ENV=development babel src/ -w -d demo-dist/src/",
    "start:prod": "NODE_ENV=production node demo-dist/demo/server/index.js",
    "build:prod": "npm run clean && npm run build:client:prod && npm run build:server:prod && npm run build:lib:prod",
    "build:client:prod": "NODE_ENV=production webpack --config demo/scripts/webpack.config.js",
    "build:server:prod": "NODE_ENV=production babel demo/ -d demo-dist/demo",
    "build:lib:prod": "NODE_ENV=production babel src/ -d demo-dist/src/",
    "delay": "node ./node_modules/npm-delay 5000",
    "copy": "mkdir -p demo-dist/public && cp favicon.ico demo-dist/public",
    "clean": "rm -rf demo-dist && rm -rf lib",
    "build:gh-pages": "NODE_ENV=production webpack --config demo/scripts/webpack.config.gh-pages.js",
    "build:lib": "run-p build:lib:js build:lib:scss",
    "build:lib:js": "NODE_ENV=production babel src/ -d lib/ -D",
    "build:lib:scss": "node-sass --output-style expanded --output lib/ src/"
  },
  "keywords": [
    "editor"
  ],
  "author": "unordered",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unordered/unordered-editor.git"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "unordered-editor-buffer": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "koa": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-static-cache": "^4.0.0",
    "node-sass": "^4.5.0",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "sass-loader": "^6.0.3",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.2",
    "unordered-react-mobile": "^1.1.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
